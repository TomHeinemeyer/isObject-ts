{
  "name": "isobject-ts",
  "version": "1.0.3",
  "author": "Tom Heinemeyer (https://github.com/TomHeinemeyer)",
  "license": "MIT",
  "description": "Determine wether something is an object, with TypeScript support.",
  "keywords": [
    "check",
    "is-object",
    "is-object-ts",
    "isobject",
    "isobjectts",
    "object",
    "type",
    "typeof",
    "value",
    "typescript"
  ],
  "homepage": "https://github.com/TomHeinemeyer/isObject-ts",
  "contributors": [
    "Tom Heinemeyer (https://github.com/TomHeinemeyer)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TomHeinemeyer/isObject-ts.git"
  },
  "bugs": {
    "url": "https://github.com/TomHeinemeyer/isObject-ts/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "/dist",
    "/src",
    "!test.ts"
  ],
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "Powershell Remove-Item -LiteralPath './dist' -Force -Recurse -ErrorAction Ignore",
    "build": "run-s -c build:clean compile",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}
